# Application
NODE_ENV=development
PORT=3000

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=gateway_db
DB_USER=gateway_user
DB_PASSWORD=gateway_pass

# JWT
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRES_IN=7d

# CORS
CORS_ORIGIN=http://localhost:4200

# Upload
UPLOAD_MAX_SIZE=104857600
UPLOAD_DEST=./uploads

# Cache
CACHE_TTL=3600

# Swagger
SWAGGER_TITLE=API Gateway Editor
SWAGGER_DESCRIPTION=API for managing API Gateway configurations
SWAGGER_VERSION=1.0.0
SWAGGER_PATH=api-docs